    detalle_pago.id_banco,
    detalle_pago.nombre_banco,
    detalle_pago.id_unidad_administrativa,
    detalle_pago.unidad_administrativa,
    sum(detalle_pago.monto_pago) AS total_deduccion_pago,
    detalle_pago.fecha_pago,
    detalle_pago.periodo_inicio,
    detalle_pago.periodo_final
   FROM detalle_pago
  WHERE (((detalle_pago.tipo)::text = 'Deduccion'::text) OR ((detalle_pago.tipo)::text = 'Credito'::text))
  GROUP BY detalle_pago.cod_empleado, detalle_pago.cedula, detalle_pago.nombres, detalle_pago.apellidos, detalle_pago.id_tipo_nomina, detalle_pago.tipo_nomina, detalle_pago.id_banco, detalle_pago.nombre_banco, detalle_pago.id_unidad_administrativa, detalle_pago.unidad_administrativa, detalle_pago.fecha_pago, detalle_pago.periodo_inicio, detalle_pago.periodo_final
  ORDER BY detalle_pago.id_unidad_administrativa, detalle_pago.cod_empleado;


ALTER TABLE total_deducciones_pago OWNER TO postgres;

--
-- Name: pago_final; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW pago_final AS
 SELECT dn.cod_empleado,
    dn.cedula,
    dn.nombres,
    dn.apellidos,
    dn.cod_partida,
    dn.partida,
    dn.tipo,
    dn.monto_pago,
    dn.id_tipo_nomina,
    dn.tipo_nomina,
    dn.id_banco,
    dn.nombre_banco,
    dn.id_unidad_administrativa,
    dn.unidad_administrativa,
    dn.fecha_pago,
    dn.periodo_inicio,
    dn.periodo_final,
    ta.total_asignacion_pago,
    COALESCE(td.total_deduccion_pago, (0)::double precision) AS total_deduccion_pago,
    COALESCE((ta.total_asignacion_pago - td.total_deduccion_pago), ta.total_asignacion_pago) AS total_neto_pago
   FROM ((detalle_pago dn
     LEFT JOIN total_asignaciones_pago ta ON ((dn.cod_empleado = ta.cod_empleado)))
     LEFT JOIN total_deducciones_pago td ON ((td.cod_empleado = dn.cod_empleado)));


ALTER TABLE pago_final OWNER TO postgres;

